{
  "manifest_version": 3,
  "name": "Logedo CRM",
  "description": "Logedo CRM Extension",
  "version": "1.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "persistent": false
  }
}